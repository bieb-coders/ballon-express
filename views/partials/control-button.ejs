<div class="col-lg-3 col-md-6 col-sm-6">
    <div class="card card-stats">
        <div class="card-header" data-background-color="green">
            <!--i class="material-icons">content_copy</i-->
            <i class="material-icons">explore</i>
        </div>
        <div class="card-content">
            <p class="category"><%= balloon.name %></p>
            <button id="<%= balloon.id %>" name="<%= balloon.name %>" type="button" class="btn btn-info command"
                <%= (balloon.used) ? "disabled" : "" %> >Plof</button>
        </div>
        <div class="card-footer">
            <div class="stats">
                <i class="material-icons">update</i>
                <span id="<%= balloon.id + '_updated' %>">
                    <%= (balloon.lastModified > 0) ? balloon.lastModified.toLocaleTimeString('nl-NL') : "never" %>
                </span>
            </div>
        </div>
    </div>
</div>