<script type="text/javascript">
    /*
    var map = L.map('map').setView([52.63275, 4.74386], 14);
    L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
      attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
    }).addTo(map);
    */
    
    $(document).ready(() => {
        var dwidth = $(window).width()*0.65;
        var dheight = $(window).height()*0.84;
        $("#imgmodal").dialog({
            autoOpen:false,
            height: dheight,
            width:dwidth,
            modal: true,
            show: {
                effect: "fade",
                duration: 500
            },
            hide: {
                effect: "fade",
                duration: 500
            }
        });
        $("#infoimg").on("click",function() {
            $("#imgmodal").dialog("open");
        });
    });

</script>

</html>